{
	"name": "vscode-open",
	"displayName": "Open",
	"description": "An extension for opening files",
	"version": "0.2.5",
	"publisher": "alexander-yu",
	"engines": {
		"vscode": "^1.43.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/alexander-yu/vscode-open"
	},
	"homepage": "https://github.com/alexander-yu/vscode-open",
	"bugs": {
		"url": "https://github.com/alexander-yu/vscode-open/issues"
	},
	"categories": [
		"Other"
	],
	"icon": "images/icon.png",
	"activationEvents": [
		"*"
	],
	"main": "./dist/extension.js",
	"keywords": [
		"open",
		"file",
		"browser"
	],
	"contributes": {
		"commands": [
			{
				"command": "vscode-open.open",
				"title": "Open: Open Current File"
			},
			{
				"command": "vscode-open.openPR",
				"title": "Open: Open Pull Request for Current Line"
			},
			{
				"command": "vscode-open.openLines",
				"title": "Open: Open Current File with Selected Lines"
			},
			{
				"command": "vscode-open.openURI",
				"title": "Open: Open File from URI"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "vscode-open.open",
					"group": "navigation"
				},
				{
					"command": "vscode-open.openLines",
					"group": "navigation"
				}
			],
			"editor/context": [
				{
					"command": "vscode-open.open",
					"group": "navigation"
				},
				{
					"command": "vscode-open.openPR",
					"group": "navigation"
				},
				{
					"command": "vscode-open.openLines",
					"group": "navigation"
				}
			]
		},
		"keybindings": [
			{
				"command": "vscode-open.open",
				"key": "ctrl+alt+o o",
				"mac": "cmd+alt+o o"
			},
			{
				"command": "vscode-open.openPR",
				"key": "ctrl+alt+o p",
				"mac": "cmd+alt+o p"
			},
			{
				"command": "vscode-open.openLines",
				"key": "ctrl+alt+o l",
				"mac": "cmd+alt+o l"
			},
			{
				"command": "vscode-open.openURI",
				"key": "ctrl+alt+o u",
				"mac": "cmd+alt+o u"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Open",
			"properties": {
				"open.fileMappings": {
					"type": "array",
					"default": [],
					"description": "File mappings"
				},
				"open.prMappings": {
					"type": "array",
					"default": [],
					"description": "Pull request mappings"
				},
				"open.uriMappings": {
					"type": "array",
					"default": [],
					"description": "URI mappings"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"compile": "webpack --mode none",
		"lint": "eslint src --ext ts",
		"watch": "webpack --mode none --watch",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js",
		"test:unittest": "nyc mocha ./out/test/unit --ui=tdd --recursive --colors --timeout=5000",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"test-compile": "tsc -p ./",
		"clean": "rm -rf ./dist ./out *.vsix .nyc_output coverage"
	},
	"devDependencies": {
		"@types/chai": "^4.2.14",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^9.0.0",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.43.0",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"chai": "^4.2.0",
		"eslint": "^8.0.1",
		"glob": "^7.1.6",
		"mocha": "^9.1.3",
		"nyc": "^15.1.0",
		"source-map-support": "^0.5.19",
		"ts-loader": "^8.0.3",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0",
		"webpack": "^5.59.0",
		"webpack-cli": "^4.9.1"
	},
	"dependencies": {
		"fp-ts": "^2.8.2",
		"io-ts": "^2.2.10",
		"simple-git": "^1.131.0",
		"yarn": "^1.22.4"
	}
}
