{
	"name": "vscode-open",
	"displayName": "Open",
	"description": "An extension for opening files",
	"version": "0.0.1",
	"publisher": "alexander-yu",
	"engines": {
		"vscode": "^1.43.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/alexander-yu/vscode-open"
	},
	"homepage": "https://github.com/alexander-yu/vscode-open",
	"bugs": {
		"url": "https://github.com/alexander-yu/vscode-open/issues"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"keywords": [
		"open",
		"file",
		"browser"
	],
	"contributes": {
		"commands": [
			{
				"command": "vscode-open.open",
				"title": "Open: Open Current File"
			},
			{
				"command": "vscode-open.openPR",
				"title": "Open: Open Pull Request for Current Line"
			},
			{
				"command": "vscode-open.openLines",
				"title": "Open: Open Current File with Selected Lines"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "vscode-open.open",
					"group": "navigation"
				},
				{
					"command": "vscode-open.openLines",
					"group": "navigation"
				}
			],
			"editor/context": [
				{
					"command": "vscode-open.open",
					"group": "navigation"
				},
				{
					"command": "vscode-open.openPR",
					"group": "navigation"
				},
				{
					"command": "vscode-open.openLines",
					"group": "navigation"
				}
			]
		},
		"keybindings": [
			{
				"command": "vscode-open.open",
				"key": "ctrl+alt+o o",
				"mac": "cmd+alt+o o"
			},
			{
				"command": "vscode-open.openPR",
				"key": "ctrl+alt+o p",
				"mac": "cmd+alt+o p"
			},
			{
				"command": "vscode-open.openLines",
				"key": "ctrl+alt+o l",
				"mac": "cmd+alt+o l"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Open",
			"properties": {
				"open.fileMappings": {
					"type": "array",
					"default": [],
					"description": "File mappings"
				},
				"open.prMappings": {
					"type": "array",
					"default": [],
					"description": "Pull request mappings"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.43.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^2.18.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.0",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"simple-git": "^1.131.0",
		"yarn": "^1.22.4"
	}
}
